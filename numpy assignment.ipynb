    "import numpy as np\n",
    "\n",
    "def mv_avg(a, n):\n",
    "    asum = np.cumsum(a, dtype=float)\n",
    "    asum[n:] = asum[n:] - asum[:-n]\n",
    "    return asum[n-1:] / n\n",
    "\n",
    "a= [3, 5, 7, 2, 8, 10, 11, 65, 72, 81, 99, 100, 150]\n",
    "a1=np.array(a)\n",
    "np.array(mv_avg(a,n=3))\n",
